root = true

[*]
charset                               = utf-8
end_of_line                           = lf
indent_size                           = 2
indent_style                          = space
tab_width                             = 2
insert_final_newline                  = true
trim_trailing_whitespace              = true
max_line_length                       = 120

ij_any_align_consecutive_assignments  = true
ij_any_align_group_field_declarations = true
